{
    "printWidth": 80,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "vueIndentScriptAndStyle": true,
    "overrides": [
        {
            "files": "src/*/.js",
            "options": {
                "semi": true
            }
        }
    ]
}
